{
  "name": "@ngrx/signals",
  "version": "17.1.1",
  "description": "Reactive Store and Set of Utilities for Angular Signals",
  "repository": {
    "type": "git",
    "url": "https://github.com/ngrx/platform.git"
  },
  "keywords": [
    "Angular",
    "NgRx",
    "Signals",
    "Signal Store",
    "Signal State",
    "State Management"
  ],
  "author": "NgRx",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ngrx/platform/issues"
  },
  "homepage": "https://github.com/ngrx/platform#readme",
  "peerDependencies": {
    "@angular/core": "^17.0.0",
    "rxjs": "^6.5.3 || ^7.4.0"
  },
  "peerDependenciesMeta": {
    "rxjs": {
      "optional": true
    }
  },
  "schematics": "./schematics/collection.json",
  "sideEffects": false,
  "ng-update": {
    "packageGroup": [
      "@ngrx/store",
      "@ngrx/effects",
      "@ngrx/entity",
      "@ngrx/router-store",
      "@ngrx/data",
      "@ngrx/schematics",
      "@ngrx/store-devtools",
      "@ngrx/component-store",
      "@ngrx/component",
      "@ngrx/eslint-plugin",
      "@ngrx/signals"
    ],
    "migrations": "./migrations/migration.json"
  },
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "module": "fesm2022/ngrx-signals.mjs",
  "typings": "index.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./index.d.ts",
      "esm2022": "./esm2022/ngrx-signals.mjs",
      "esm": "./esm2022/ngrx-signals.mjs",
      "default": "./fesm2022/ngrx-signals.mjs"
    },
    "./entities": {
      "types": "./entities/index.d.ts",
      "esm2022": "./esm2022/entities/ngrx-signals-entities.mjs",
      "esm": "./esm2022/entities/ngrx-signals-entities.mjs",
      "default": "./fesm2022/ngrx-signals-entities.mjs"
    },
    "./rxjs-interop": {
      "types": "./rxjs-interop/index.d.ts",
      "esm2022": "./esm2022/rxjs-interop/ngrx-signals-rxjs-interop.mjs",
      "esm": "./esm2022/rxjs-interop/ngrx-signals-rxjs-interop.mjs",
      "default": "./fesm2022/ngrx-signals-rxjs-interop.mjs"
    }
  }
}